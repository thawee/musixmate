<?xml version="1.0" encoding="utf-8"?>
<resources>

	<attr name="primaryTextSelector" format="reference" />

	<declare-styleable name="ProgressBar">
		<attr name="barColor" format="color" />
		<attr name="barHeight" format="dimension" />
		<attr name="detentWidth" format="dimension" />
		<attr name="useShadow" format="boolean" />
	</declare-styleable>

	<declare-styleable name="ForegroundView">
		<attr name="foreground" />
		<attr name="foregroundInsidePadding" />
		<attr name="foregroundGravity" />
	</declare-styleable>

	<declare-styleable name="CollapsingImageBehavior">
		<attr name="collapsedTarget" format="reference" />
	</declare-styleable>

    <!-- CarouselMenuView -->
    <declare-styleable name="CarouselMenuView">
        <attr name="items_visible" />
    </declare-styleable>
    <attr name="items_visible" format="enum">
        <enum name="three" value="3" />
        <enum name="five" value="5" />
        <enum name="seven" value="7" />
    </attr>

	<declare-styleable name="AvatarImageBehavior">
		<attr name="finalYPosition" format="dimension"/>
		<attr name="startXPosition" format="dimension"/>
		<attr name="startToolbarPosition" format="dimension"/>
		<attr name="startHeight" format="dimension"/>
		<attr name="finalHeight" format="dimension"/>
	</declare-styleable>

</resources>