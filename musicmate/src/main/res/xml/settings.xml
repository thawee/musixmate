<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/preferences_sub_tags">
        <MultiSelectListPreference
            android:entries="@array/MetaDataEncodingNames"
            android:title="@string/MetaDataEncoding"
            android:key="preference_matadata_encodings"
            android:summary="@string/MetaDataEncodingSummary"
            android:dialogTitle="@string/MetaDataEncoding"
            android:entryValues="@array/MetaDataEncodingValues"
            android:defaultValue="@array/MetaDataEncodingValues"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/preferences_sub_advanced">
        <CheckBoxPreference
            android:key="preference_notification"
            android:summary="Notification on Now Listening song."
            android:title="Enable Notification"
            android:defaultValue="true"/>
    </PreferenceCategory>
</PreferenceScreen>